    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-loader": "^8.2.3",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.2.0",
    "ts-node": "^10.4.0",
